{
	"name": "ExcelSource",
	"properties": {
		"linkedServiceName": {
			"referenceName": "pocexcelstorageaccount",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "Example Charge data.xlsx"
			},
			"Container": {
				"type": "string",
				"defaultValue": "test"
			},
			"FirstRowAsHeader": {
				"type": "bool",
				"defaultValue": true
			}
		},
		"annotations": [],
		"type": "Excel",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@{dataset().FileName}",
					"type": "Expression"
				},
				"container": {
					"value": "@{dataset().Container}",
					"type": "Expression"
				}
			},
			"firstRowAsHeader": true
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}